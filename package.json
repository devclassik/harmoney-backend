{
  "name": "Harmoney-backend",
  "version": "1.0.0",
  "description": "Backend service for Harmoney",
  "main": "src/app.ts",
  "author": "Heimdall Inc.",
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/devclassik/harmoney-backend",
  "scripts": {
    "build": "rm -rf build && tsc",
    "start": "node -r tsconfig-paths/register -r dotenv/config build/app.js",
    "reg": "ts-node -r tsconfig-paths/register src/app.ts",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/app.ts",
    "dev:js": "nodemon -r dotenv/config tsconfig-paths/register --exec node build/app.js",
    "compile": "./node_modules/.bin/tsc --project ./tsconfig.json",
    "compile:noEmit": "./node_modules/.bin/tsc --project ./tsconfig.json --noEmit",
    "serve:js": "node build/app.js",
    "npm:clear": "rm -rf node_modules/ && echo \"Cleared node modules\""
  },
  "dependencies": {
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.1",
    "firebase": "^10.12.3",
    "firebase-admin": "^12.2.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^10.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "nodemailer": "^6.9.15",
    "pg": "^8.12.0",
    "qrcode": "^1.5.3",
    "socket.io": "^4.8.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.5.3",
    "unique-names-generator": "^4.7.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.14.10",
    "@types/nodemailer": "^6.4.16",
    "@types/qrcode": "^1.5.5",
    "nodemon": "^3.1.4"
  }
}
